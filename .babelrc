{
    "plugins": [
      [
        "module-resolver",
        {
          "cwd": "babelrc",
          "root": ["./src"],
          "extensions": [".js"],
          "alias": {
            "_assets": "./src/assets",
            "_contexts": "./src/contexts",
            "_hooks": "./src/hooks",
            "_constants": "./src/constants",
            "_models": "./src/models",
            "_components": "./src/components",
            "_navigations": "./src/navigations",
            "_scenes": "./src/scenes",
            "_services": "./src/services",
            "_styles": "./src/styles",
            "_utils": "./src/utils"
          }
        }
      ]
    ]
  }